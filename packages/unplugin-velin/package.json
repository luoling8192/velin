  "scripts": {
    "dev": "unbuild",
    "stub": "unbuild",
    "build": "unbuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@velin-dev/core": "workspace:^",
    "defu": "^6.1.4",
