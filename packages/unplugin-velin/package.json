{
  "name": "@velin-dev/unplugin-velin",
  "type": "module",
  "version": "0.0.5",
  "description": "Develop prompts with Vue SFC or Markdown like pro.",
  "author": {
    "name": "RainbowBird",
    "email": "rbxin2003@outlook.com",
    "url": "https://github.com/luoling819"
  },
  "license": "MIT",
  "homepage": "https://github.com/luoling819/velin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luoling819/velin.git",
    "directory": "packages/core"
  },
  "bugs": "https://github.com/luoling819/velin/issues",
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs"
      },
      "./vite": {
        "types": "./dist/vite.d.ts",
        "import": "./dist/vite.mjs"
      },
      "./types": {
        "types": "./dist/types/index.d.ts"
      }
    },
    "main": "dist/index.mjs",
    "types": "dist/index.d.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./vite": "./src/vite.ts",
    "./types": "./src/types/index.ts"
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "pkgroll",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@velin-dev/core": "workspace:^",
    "defu": "^6.1.4",
    "path-browserify-esm": "^1.0.6",
    "unplugin": "^2.3.2"
  }
}
